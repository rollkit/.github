# DO NOT EDIT DIRECTLY
# EDIT TEMPLATE IN ROLLKIT/.GITHUB REPO
version: 2
updates:
{% for dep in deps %}
  - package-ecosystem: {{dep.ecosystem}}
    directory: "/"
    schedule:
      interval: weekly
    open-pull-requests-limit: 10
    groups:
      patch-updates:
        applies-to: version-updates
        update-types:
          - "patch"
          - "minor"
    commit-message:
      include: "scope"
      prefix: "build"
{% endfor %}
